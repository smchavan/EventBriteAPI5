networks:
  backend: {}
  frontend: {}
services:
  basket.data:
    image: redis
    networks:
      backend: null
    ports:
    - published: 6378
      target: 6379
  cart:
    build:
      context: C:\Users\smvnc\source\repos\EventBriteAPI5\CartApi
      dockerfile: Dockerfile
    container_name: cartapi
    depends_on:
    - basket.data
    - tokenserver
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ConnectionString: basket.data
      IdentityUrl: http://192.168.1.22:5005
    image: events/cartapi
    networks:
      backend: null
      frontend: null
    ports:
    - published: 5005
      target: 80
  event:
    build:
      context: C:\Users\smvnc\source\repos\EventBriteAPI5\EventCatalogAPI
      dockerfile: Dockerfile
    container_name: eventapi
    depends_on:
    - mssqlserver
    environment:
      DatabaseName: EventDb
      DatabaseServer: mssqlserver
      DatabaseUser: sa
      DatabaseUserPassword: EventApi(!)
      ExternalEventBaseUrl: http://localhost:5060
    image: events/eventapi
    networks:
      backend: null
      frontend: null
    ports:
    - published: 5060
      target: 80
  mssqlserver:
    container_name: eventdb
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: EventApi(!)
    image: microsoft/mssql-server-linux:latest
    networks:
      backend: null
    ports:
    - published: 1448
      target: 1433
  order:
    build:
      context: C:\Users\smvnc\source\repos\EventBriteAPI5\OrderApi
      dockerfile: Dockerfile
    container_name: orderapi
    depends_on:
    - mssqlserver
    - tokenserver
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DATABASE: OrdersDb
      IdentityUrl: http://172.20.10.8:5065
      SA_PASSWORD: EventApi(!)
      SQLSERVER_HOST: eventdb
    image: events/orderapi
    networks:
      backend: null
      frontend: null
    ports:
    - published: 5065
      target: 80
  tokenserver:
    build:
      context: C:\Users\smvnc\source\repos\EventBriteAPI5\TokenServiceApi
      dockerfile: Dockerfile
    container_name: tokenserviceapi1
    depends_on:
    - mssqlserver
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      MvcClient: http://localhost:5045
    image: events/tokenapi
    networks:
      backend: null
      frontend: null
    ports:
    - published: 5015
      target: 80
  webmvc:
    build:
      context: C:\Users\smvnc\source\repos\EventBriteAPI5\webmvc
      dockerfile: Dockerfile
    container_name: webshop1
    depends_on:
    - cart
    - event
    - order
    - tokenserver
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      CallBackUrl: http://localhost:5045
      CartUrl: http://cart
      EventUrl: http://event
      IdentityUrl: 192.168.1.22:5015
      OrderUrl: http://order
      StripePrivateKey: sk_test_NVXfiXyRDmTZVge5c3Zza7Fe00cHgjPL9M
      StripePublicKey: pk_test_GJtMaolsgxI5jQtkuq8rDdjD00qbRyn2e8
    image: events/webmvc
    networks:
      frontend: null
    ports:
    - published: 5045
      target: 80
version: '3.4'