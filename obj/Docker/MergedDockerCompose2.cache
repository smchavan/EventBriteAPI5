networks:
  backend: {}
  frontend: {}
services:
  event:
    build:
      context: C:\Users\smvnc\source\repos\EventBriteAPI5\EventCatalogAPI
      dockerfile: Dockerfile
    container_name: eventapi
    depends_on:
    - mssqlserver
    environment:
      DatabaseName: EventDb
      DatabaseServer: mssqlserver
      DatabaseUser: sa
      DatabaseUserPassword: EventApi(!)
      ExternalCatalogBaseUrl: http://localhost:5060
    image: events/eventapi
    networks:
      backend: null
      frontend: null
    ports:
    - published: 5060
      target: 80
  mssqlserver:
    container_name: eventdb
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: EventApi(!)
    image: microsoft/mssql-server-linux:latest
    networks:
      backend: null
    ports:
    - published: 1448
      target: 1433
  webmvc:
    build:
      context: C:\Users\smvnc\source\repos\EventBriteAPI5\webmvc
      dockerfile: Dockerfile
    container_name: webshop
    depends_on:
    - event
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      EventUrl: http://event
    image: events/webmvc
    networks:
      frontend: null
    ports:
    - published: 5090
      target: 80
version: '3.4'